{
	"name": "vscode-corda",
	"displayName": "vscode-corda",
	"publisher": "R3",
	"description": "Corda Tools",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:build.gradle"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.cordaClean",
				"title": "Corda Clean project"
			},
			{
				"command": "extension.cordaBuild",
				"title": "Corda Build project"
			},
			{
				"command": "extension.cordaTest",
				"title": "Corda Run Tests"
			},
			{
				"command": "extension.cordaDeployNodes",
				"title": "Corda Deploy Nodes"
			},
			{
				"command": "extension.cordaRunNodes",
				"title": "Corda Run Nodes"
			},
			{
				"command": "extension.launchServer",
				"title": "Launch the server"
			},
			{
				"command" : "extension.cordaShowVaultQuery",
				"title" : "Corda Show Vault Query View"
			},
			{
				"command" : "extension.cordaShowTransactionExplorer",
				"title" : "Corda Show Transaction Explorer"
			},
			{
				"command": "extension.launchClient",
				"title": "Launch the client"
			},
			{
				"command": "extension.investigateNode",
				"title": "Investigate the nodes"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "npm-run-all compile:*",
		"compile:extension": "tsc -p ./",
		"compile:views": "webpack --mode development",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"watch:views": "webpack --watch --mode development"
	},
	"devDependencies": {
		"@babel/core": "^7.1.2",
		"@babel/preset-env": "^7.1.0",
		"@babel/preset-react": "^7.0.0",
		"@types/find": "^0.2.1",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"babel-loader": "^8.0.4",
		"file-loader": "^4.2.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"npm-run-all": "^4.1.3",
		"react-inlinesvg": "^1.1.5",
		"svg-inline-loader": "^0.8.0",
		"ts-import-plugin": "^1.5.4",
		"ts-loader": "^4.4.2",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vsce": "^1.47.0",
		"vscode-test": "^1.0.0-next.0",
		"webpack": "^4.15.1",
		"webpack-cli": "^3.0.8"
	},
	"dependencies": {
		"@babel/core": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-react": "^7.0.0",
		"@material-ui/core": "^4.3.3",
		"@material-ui/icons": "^4.2.1",
		"axios": "^0.19.0",
		"babel-loader": "^8.0.6",
		"find": "^0.3.0",
		"fs": "0.0.1-security",
		"gradle-to-js": "^2.0.0",
		"konva": "^4.0.5",
		"lodash.merge": "^4.6.2",
		"material-ui-icons": "^1.0.0-beta.36",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"react-konva": "^16.9.0-0",
		"react-scripts": "3.0.1",
		"react-tooltip": "^3.10.0",
		"stream": "0.0.2"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"eslintConfig": {
		"extends": "react-app"
	}
}
